{
  "name": "vscode-sourcemaps-navigator",
  "displayName": "vscode-sourcemaps-navigator",
  "description": "An extension to navigate to original source of compiled/transpiled javascript code",
  "version": "0.0.1",
  "publisher": "vladimir-kotikov",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:smnavigator.navigate"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "smnavigator.navigate",
        "title": "Source maps: Navigate"
      }
    ],
    "keybindings": [
      {
        "command": "smnavigator.navigate",
        "key": "shift+f7",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/source-map": "^0.5.0",
    "mocha": "^2.3.3",
    "typescript": "^2.1.5",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "source-map": "^0.5.6"
  }
}
